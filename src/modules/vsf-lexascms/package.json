{
  "name": "vsf-lexascms",
  "version": "0.1.3",
  "description": "Official LexasCMS module for Vue Storefront.",
  "keywords": [
    "vue-storefront",
    "lexascms"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/LexasCMS/vsf-lexascms.git"
  },
  "license": "MIT",
  "author": "LexasCMS (https://www.lexascms.com)",
  "dependencies": {
    "axios": "^0.21.1",
    "base-64": "^1.0.0",
    "jsona": "^1.8.0",
    "qs": "^6.9.4"
  },
  "devDependencies": {
    "@types/base-64": "^0.1.3",
    "@types/qs": "^6.9.4",
    "release-it": "^14.0.2",
    "release-it-lerna-changelog": "^2.4.0"
  },
  "peerDependencies": {
    "vue": "^2.6.11"
  },
  "release-it": {
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    },
    "npm": {
      "publish": false
    },
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": true
      }
    }
  }
}
